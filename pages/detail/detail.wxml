
<view class="body">
<map id='map' 
        longitude="{{longitude}}" 
        latitude="{{latitude}}" 
        scale="17"  
        markers="{{markers}}" 
        polygons="{{polygons}}"
        enable-satellite
        style='width:100%;height:35vh;'>
    <cover-view slot="callout">
      <cover-view marker-id="{{customCalloutInfo.id}}" slot="callout" class="areainfo">
        <cover-view class="areainfo-body">
            <cover-view class="areainfo-body-cell">
              <cover-image class="areainfo-body-cell-img" src="../../images/infotag1.png"></cover-image>
              <cover-view class="areainfo-body-cell-info">
                <cover-view class="areainfo-body-cell-info-title">{{customCalloutInfo.install_edcapacity}}</cover-view>
                <cover-view class="areainfo-body-cell-info-subtitle">总装机量(MW)</cover-view>
              </cover-view>
            </cover-view>
            <cover-view class="areainfo-body-cell body-cell-left">
              <cover-image class="areainfo-body-cell-img cell-img-left"  src="../../images/infotag2.png"></cover-image>
              <cover-view class="areainfo-body-cell-info">
                <cover-view class="areainfo-body-cell-info-title">{{customCalloutInfo.sum_price}}万</cover-view>
                <cover-view class="areainfo-body-cell-info-subtitle">总投资(元)</cover-view>
              </cover-view>
            </cover-view>
            <cover-view class="areainfo-body-cell body-cell-top">
              <cover-image class="areainfo-body-cell-img"  src="../../images/infotag3.png"></cover-image>
              <cover-view class="areainfo-body-cell-info">
                <cover-view class="areainfo-body-cell-info-title">{{customCalloutInfo.area}}</cover-view>
                <cover-view class="areainfo-body-cell-info-subtitle">面积(㎡)</cover-view>
              </cover-view>
            </cover-view>
            <cover-view class="areainfo-body-cell body-cell-left body-cell-top">
              <cover-image class="areainfo-body-cell-img cell-img-left"  src="../../images/infotag4.png"></cover-image>
              <cover-view class="areainfo-body-cell-info">
                <cover-view class="areainfo-body-cell-info-title">{{customCalloutInfo.year_generating_capacity}}万</cover-view>
                <cover-view class="areainfo-body-cell-info-subtitle">年发电量(度)</cover-view>
              </cover-view>
            </cover-view>
            <cover-view class="areainfo-body-cell areainfo-body-cell-button" >
              查看详情
            </cover-view>
            <!-- <cover-view class="areainfo-body-cell areainfo-body-cell-button areainfo-body-cell-width">
              去分享
            </cover-view> -->
        </cover-view>
        <cover-view class="areainfo-foot">
          <cover-image class="areainfo-foot-img" src="../../images/down.png"></cover-image>
        </cover-view>
      </cover-view>
    </cover-view>
    
  </map> 
  <view class="body-cell">
    <view class="cell1 cell-t-l">装机容量</view>
    <view class="cell1 cell-t-c">每瓦成本</view>
    <view class="cell1 cell-t-r">投资成本</view>
  </view>
  <view class="body-cell">
    <view class="cell1 cell-t-l">{{install_area}}MW ×</view>
    <view class="cell1 cell-t-c">
      <input class="cell1-inp" type="digit" maxlength="6" bindfocus="w_cost_inp_focus" bindblur="w_cost_inp_blur" value="{{w_cost}}"/>
    </view>
    <view class="cell1 cell-t-r">¥ {{cost}}万</view>
  </view>
  <view class="body-cell">
    <view class="cell2 cell-t-l">安装面积(m²)</view>
    <view class="cell2 cell-t-r">{{area}}</view>
  </view>
  <view class="body-cell">
    <view class="cell2 cell-t-l">辐照参数(kWh/kWp/年)</view>
    <view class="cell2 cell-t-r">{{year_light}}</view>
  </view>
  <view class="body-cell">
    <view class="cell2 cell-t-l">年发电量(度)</view>
    <view class="cell2 cell-t-r">{{generating_year}}万</view>
  </view>

  <view class="body-cell">
    <view class="cell2 cell-t-l">国家补贴(元/瓦)</view>
    <view class="cell2 cell-t-r">
      <input class="cell2-inp" type="digit" maxlength="6" bindfocus="country_subsidy_inp_focus" bindblur="country_subsidy_inp_blur" value="{{country_subsidy}}"/>
    </view>
  </view>
  <view class="body-cell">
    <view class="cell2 cell-t-l">地方补贴(元/瓦)</view>
    <view class="cell2 cell-t-r">
      <input class="cell2-inp" type="digit" maxlength="6" bindfocus="local_subsidy_inp_focus" bindblur="local_subsidy_inp_blur" value="{{local_subsidy}}"/>
    </view>
  </view>
  <view class="body-cell">
    <view class="cell2 cell-t-l">初装补贴(元/瓦)</view>
    <view class="cell2 cell-t-r">
      <input class="cell2-inp" type="digit" maxlength="6" bindfocus="init_subsidy_inp_focus" bindblur="init_subsidy_inp_blur" value="{{init_subsidy}}"/>
    </view>
  </view>
  <view class="body-cell">
    <view class="cell2 cell-t-l">度电收益(元/瓦)</view>
    <view class="cell2 cell-t-r">
      <input class="cell2-inp" type="digit" maxlength="6" bindfocus="yield_inp_focus" bindblur="yield_inp_blur" value="{{yield}}"/>
    </view>
  </view>
  <view class="body-cell">
    <view class="cell2 cell-t-l">租金(元/平方米/年)</view>
    <view class="cell2 cell-t-r">
      <input class="cell2-inp" type="digit" maxlength="6" bindfocus="rent_inp_focus" bindblur="rent_inp_blur" value="{{rent}}"/>
    </view>
  </view>
  <view class="body-cell">
    <view class="cell2 cell-t-l">运维成本(元/W/年)</view>
    <view class="cell2 cell-t-r">
      <input class="cell2-inp" type="digit" maxlength="6" bindfocus="operational_cost_inp_focus" bindblur="operational_cost_inp_blur" value="{{operational_cost}}"/>
    </view>
  </view>

  <view class="body-cell">
    <view class="cell2 cell-t-l">年收益(万元)</view>
    <view class="cell2 cell-t-r">{{yield_year}}万</view>
  </view>
  <view class="body-cell">
    <view class="cell2 cell-t-l">税前年收益(%)</view>
    <view class="cell2 cell-t-r">{{pre_tax_yield_year}}</view>
  </view>
  <view class="body-cell">
    <view class="cell2 cell-t-l">回本周期(/年)</view>
    <view class="cell2 cell-t-r">{{back_period}}</view>
  </view>
</view>

<view class="baocun" bindtap="updetail">
  保存
</view>
